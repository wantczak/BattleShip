<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="menu" prefHeight="379.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="battleship.gui.menu.MenuViewController">
   <left>
      <VBox alignment="TOP_CENTER" prefHeight="341.0" prefWidth="200.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnStartGame" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Rozpocznij nową grę" />
            <Button fx:id="btnJoinGame" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Dołącz do gry" />
            <Button fx:id="btnClose" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Zakończ" />
            <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="198.0" text="Informacje" />
         </children>
         <BorderPane.margin>
            <Insets left="5.0" right="5.0" top="10.0" />
         </BorderPane.margin></VBox>
   </left>
   <center>
      <BorderPane fx:id="contentPane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="25.0" />
         </BorderPane.margin></BorderPane>
   </center>
   <top>
      <HBox prefHeight="28.0" prefWidth="624.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnMinimize" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onMouseClicked="#btnMinimizeEvent" prefHeight="30.0" prefWidth="30.0">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="/pictures/btnMinimize.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btnClose2" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onMouseClicked="#btnCloseEvent" prefHeight="30.0" prefWidth="30.0">
               <graphic>
                  <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="/pictures/btnClose.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets right="5.0" />
         </BorderPane.margin></HBox>
   </top>
